def book_list_view(library):
    if not library:
        print('В библиотеке нет книг')
    for book in library:
        print({book})


library = {
    'Horus Rising': {
        'autor': 'Den Abnet',
        'year': '2006',
        'availability': 'true'
    },
    'False gods': {
        'autor': 'Graham McNeill',
        'year': '2006',
        'availability': 'true'
    },
    'Galaxy in Flames': {
        'autor': 'Ben Counter',
        'year': '2006',
        'availability': 'true'
    },
}


book_list_view(library)
